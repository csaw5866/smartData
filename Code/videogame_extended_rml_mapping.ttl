
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix schema: <http://schema.org/>.
@prefix ex: <http://example.org/>.

<#VideoGameMapping>
    a rr:TriplesMap;
    rml:logicalSource [
        rml:source "/path/to/imdb-videogames.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://example.org/videogame/{id}";
        rr:class schema:VideoGame
    ];
    rr:predicateObjectMap [
        rr:predicate schema:name;
        rr:objectMap [ rml:reference "name" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:url;
        rr:objectMap [ rml:reference "url" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:datePublished;
        rr:objectMap [ rml:reference "year" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:contentRating;
        rr:objectMap [ rml:reference "certificate" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:aggregateRating;
        rr:objectMap [ rml:reference "rating" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:interactionStatistic;
        rr:objectMap [ rml:reference "votes" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:description;
        rr:objectMap [ rml:reference "plot" ]
    ];

    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Action" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Adventure" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Comedy" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Crime" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Family" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Fantasy" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Mystery" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Sci-Fi" ]
    ];
    
    rr:predicateObjectMap [
        rr:predicate schema:genre;
        rr:objectMap [ rml:reference "Thriller" ]
    ];
    